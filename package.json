{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "run-s build:posts build:feed build:next",
    "build:posts": "ts-node --project tsconfig.builder.json ./src/builder/posts.ts",
    "build:feed": "NODE_ENV=production ts-node --project tsconfig.builder.json ./src/builder/feed.ts",
    "build:next": "next build",
    "start": "next start",
    "export": "next export",
    "typecheck": "tsc --noEmit",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "dayjs": "^1.9.3",
    "next": "^13.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.2",
    "@types/node": "^14.11.10",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "feed": "^4.2.2",
    "fs-extra": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rss-parser": "^3.9.0",
    "sass": "^1.27.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
